package bolao;
public class Jogador extends Pessoa {
    
    public void listarDados(String nome, String cpf, String pix) {
        for (Pessoa pessoa : getListaPessoa()) {
            if (pessoa.getnome().equals(nome) && pessoa.getcpf().equals(cpf)) {
                System.out.println(pessoa.getnome() + " - " + pessoa.getcpf() + " - " + pix);
            }
        }
    }
}
