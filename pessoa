package bolao;
import java.util.ArrayList;
import java.util.List;

    public class Pessoa {
    private String nome;
    private String cpf;
    public Pessoa(String nome, String cpf) {
        this.nome = nome;
        this.cpf = cpf;
    }
    
    public class RegistroInformacoes {
    private List<Pessoa> listaPessoa = new ArrayList<>();

    // Método para adicionar informações na lista
    public void adicionarInformacao(String parametro1, String parametro2) {
        Pessoa pessoa = new Pessoa(parametro1, parametro2);
        listaPessoa.add(pessoa);
    }

    public List<Pessoa> getListaInformacoes() {
        return listaPessoa;
    }

    public void setListaPessoa(List<Pessoa> listaPessoa) {
        this.listaPessoa = listaPessoa;
    }
}
    
    public String getnome() {
        return nome;
    }
    
    public void setnome(String nome) {
        this.nome = nome;
    }

    public String getcpf() {
        return cpf;
    }

    public void setcpf(String cpf) {
        this.cpf = cpf;
    }
}
